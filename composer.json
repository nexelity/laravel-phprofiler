{
  "name": "phprofiler/laravel-phprofiler",
  "description": "Laravel middleware to capture PHProfiler profiling data",
  "license": "Apache-2.0",
  "type": "library",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "illuminate/http": "^9.0|^10.0|^11.0",
    "illuminate/routing": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "PHProfiler\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PHProfiler\\PHProfilerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}